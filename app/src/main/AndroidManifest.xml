<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.labour.homie">

    <application
        android:allowBackup="true"
        android:icon="@drawable/h"
        android:label="@string/app_name"
        android:roundIcon="@drawable/h"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Messaging"></activity>
        <activity
            android:name=".Signupforuser"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Signupforlabour"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".BaseActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".LabourProfile"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ListOfLabours"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".UserPage"
            android:label="@string/title_activity_user_page"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>